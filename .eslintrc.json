{
  "env": {
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": [
      "warn",
      {
        "allow": ["warn", "info", "error"]
      }
    ],
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0
  }
}

